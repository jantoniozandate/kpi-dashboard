[MASTER]
ignore=venv
extension-pkg-whitelist=
init-hook='import sys; sys.path.append(".")'

[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    too-many-arguments,
    too-many-locals,
    too-many-branches,
    too-many-statements,
    too-many-instance-attributes,
    fixme,
    broad-except

[FORMAT]
max-line-length=120

[TYPECHECK]
ignored-modules=readline

[DESIGN]
max-args=6
